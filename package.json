{
  "name": "group-car-api",
  "version": "0.1.8",
  "private": true,
  "config": {
    "port": "8080",
    "public": "../group-car/build/"
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register --transpile-only src/group-car.ts",
    "debug": "set DEBUG=group-car:* & ts-node --transpile-only -r tsconfig-paths/register src/group-car.ts",
    "debugAll": "set DEBUG=* & ts-node --transpile-only -r tsconfig-paths/register src/group-car.ts",
    "eslint": "./node_modules/.bin/eslint . --ext .js,.ts",
    "tsc": "tsc",
    "prod": "tsc & node -r tsconfig-paths/register ./build/group-car.js",
    "typedoc": "typedoc",
    "test": "set DEBUG=group-car:* && mocha -r ts-node/register -r tsconfig-paths/register test/**/*.spec.ts",
    "unitTest": "set DEBUG=group-car:* mocha -r ts-node/register -r tsconfig-paths/register test/unit/**/*.spec.ts",
    "integrationTest": " set DEBUG=group-car:* && mocha -r ts-node/register -r tsconfig-paths/register test/integration/**/*.spec.ts",
    "devMigrate": "set NODE_ENV=development && sequelize-cli db:migrate",
    "prodMigrate": "set NODE_ENV=production && sequelize-cli db:migrate"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.5",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^8.0.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.6",
    "supports-color": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^7.1.1",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.17.4"
  }
}
