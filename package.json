{
  "name": "group-car-api",
  "version": "0.5.0-alpha.1",
  "private": true,
  "config": {
    "port": "8080",
    "public": "../group-car/build/"
  },
  "nyc": {
    "include": [
      "app/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "json-summary"
    ],
    "report-dir": "./static/test/coverage",
    "all": true
  },
  "scripts": {
    "start": "nodemon --exec ts-node -r tsconfig-paths/register --transpile-only app/group-car.ts",
    "dev": "env-cmd cross-env JWT_SECRET=secret yarn start",
    "dev:log": "env-cmd cross-env DEBUG=group-car* yarn dev",
    "dev:log:all": "env-cmd cross-env DEBUG=* yarn dev",
    "dev:beta:log": "env-cmd cross-env DEBUG=group-car* SERVER_TYPE=beta yarn dev",
    "eslint": "./node_modules/.bin/eslint . --ext .js,.ts",
    "eslint:fix": "./node_modules/.bin/eslint . --ext .js,.ts --fix",
    "tsc": "tsc",
    "prod": "tsc & cross-env NODE_ENV=production node -r tsconfig-paths/register ./build/group-car.js",
    "typedoc": "typedoc --out static/doc/typedoc --mode modules app",
    "test": "cross-env NODE_ENV=test JWT_SECRET=secret mocha \"app/**/*.spec.ts\"",
    "test:unit": "cross-env NODE_ENV=test JWT_SECRET=secret mocha \"app/**/*.unit.spec.ts\"",
    "test:integration": "cross-env NODE_ENV=test JWT_SECRET=secret mocha \"app/**/*.integration.spec.ts\"",
    "migrate:dev": "cross-env NODE_ENV=development sequelize-cli db:migrate",
    "migrate:prod": "cross-env NODE_ENV=production sequelize-cli db:migrate",
    "coverage": "cross-env NODE_ENV=test JWT_SECRET=secret nyc mocha \"app/**/*.spec.ts\" --exit --reporter mochawesome --reporter-options quiet=true,html=false",
    "postcoverage": "yarn marge",
    "marge": "marge -f index.html -o static/test/ -p Report mochawesome-report/mochawesome.json",
    "generate-disclaimer": "yarn licenses generate-disclaimer >> static/license-disclaimer"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/csrf": "^1.3.2",
    "@types/csurf": "^1.9.36",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.5",
    "@types/express-handlebars": "^3.1.0",
    "@types/express-jwt": "0.0.42",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/nodemailer": "^6.4.0",
    "@types/sequelize": "^4.28.8",
    "@types/sinon": "^9.0.0",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.0.0",
    "@types/yargs": "^15.0.5",
    "bcrypt": "^5.0.0",
    "canvas": "^2.6.1",
    "chai-as-promised": "^7.1.1",
    "cookie-parser": "~1.4.4",
    "csrf": "^3.1.0",
    "csurf": "^1.11.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-handlebars": "^4.0.4",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "morgan-debug": "^2.0.0",
    "nodemailer": "^6.4.10",
    "nodemailer-express-handlebars": "^4.0.0",
    "nodemailer-express-handlebars-plaintext-inline-ccs": "^0.9.0",
    "pg": "^8.0.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.6",
    "sinon": "^9.0.2",
    "socket.io": "^2.3.0",
    "supports-color": "^7.1.0",
    "typescript": "^3.8.3",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/cookie": "^0.3.3",
    "@types/mocha": "^7.0.2",
    "@types/socket.io-client": "^1.4.34",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-tsdoc": "^0.2.5",
    "mocha": "^7.1.1",
    "mochawesome": "^6.1.0",
    "mochawesome-report-generator": "^5.1.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1",
    "sequelize-cli": "^5.5.1",
    "socket.io-client": "^2.3.1",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.17.4"
  }
}
