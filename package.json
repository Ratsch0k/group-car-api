{
  "name": "group-car-api",
  "version": "0.1.6",
  "private": true,
  "config": {
    "port": "8080",
    "public": "../group-car/build/"
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register --transpile-only src/group-car.ts",
    "debug": "set DEBUG=group-car:* & ts-node-dev --respawn --transpileOnly src/group-car.ts",
    "debugAll": "set DEBUG=* & ts-node-dev --respawn --transpileOnly src/group-car.ts",
    "eslint": "./node_modules/.bin/eslint . --ext .js,.ts",
    "tsc": "tsc",
    "prod": "tsc & node ./build/group-car.js",
    "typedoc": "typedoc",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register test/**/*.spec.ts",
    "unitTest": "mocha -r ts-node/register -r tsconfig-paths/register test/unit/**/*.spec.ts",
    "integrationTest": "mocha -r ts-node/register -r tsconfig-paths/register test/integration/**/*.spec.ts"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.5",
    "@types/morgan": "^1.9.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^7.1.1",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.17.4"
  }
}
