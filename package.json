{
  "name": "group-car-api",
  "version": "0.1.15",
  "private": true,
  "config": {
    "port": "8080",
    "public": "../group-car/build/"
  },
  "nyc": {
    "include": [
      "app/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "report-dir": "./static/test/coverage",
    "all": true
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register --transpile-only app/group-car.ts",
    "debug": "cross-env DEBUG=group-car:* ts-node --transpile-only -r tsconfig-paths/register app/group-car.ts",
    "debugAll": "cross-env DEBUG=* ts-node --transpile-only -r tsconfig-paths/register app/group-car.ts",
    "eslint": "./node_modules/.bin/eslint . --ext .js,.ts",
    "tsc": "tsc",
    "prod": "tsc & cross-env NODE_ENV=production node -r tsconfig-paths/register ./build/group-car.js",
    "typedoc": "typedoc",
    "test": "cross-env NODE_ENV=test JWT_SECRET=secret mocha app/**/*.spec.ts",
    "unitTest": "cross-env NODE_ENV=test JWT_SECRET=secret mocha app/**/*.unit.spec.ts",
    "integrationTest": "cross-env NODE_ENV=test JWT_SECRET=secret mocha app/**/*.integration.spec.ts",
    "devMigrate": "cross-env NODE_ENV=development sequelize-cli db:migrate",
    "prodMigrate": "cross-env NODE_ENV=production sequelize-cli db:migrate",
    "coverage": "cross-env NODE_ENV=test JWT_SECRET=secret nyc mocha app/**/*.ts --exit --reporter-options quiet=true,html=false",
    "marge": "marge -f index.html -o static/test/ -p Report mochawesome-report/mochawesome.json"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/cookie-parser": "^1.4.2",
    "@types/csrf": "^1.3.2",
    "@types/csurf": "^1.9.36",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.5",
    "@types/express-jwt": "0.0.42",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/sequelize": "^4.28.8",
    "@types/sinon": "^9.0.0",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.0.0",
    "bcrypt": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "csrf": "^3.1.0",
    "csurf": "^1.11.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-jwt": "^5.3.3",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pg": "^8.0.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.6",
    "sinon": "^9.0.2",
    "supports-color": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^7.1.1",
    "mochawesome": "^6.1.0",
    "mochawesome-report-generator": "^5.1.0",
    "nyc": "^15.0.1",
    "sequelize-cli": "^5.5.1",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.17.4"
  }
}
