    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "ts-node-dev": "^1.0.0-pre.44"
  }
